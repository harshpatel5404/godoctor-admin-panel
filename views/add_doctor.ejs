                                        </div>
                                        <div class="col-sm-12 col-md-6 col-lg-2 col-xl-2">
                                            <label class="col-form-label"><%= lan.Clinic_Visit %> <%= lan.Price %></label>
                                            <input class="form-control" type="number" name="client_vis_price" value="" readonly>
                                        </div>
                                        <div class="col-sm-12 col-md-6 col-lg-2 col-xl-2">
                                            <label class="col-form-label"><%= lan.Video_Consult %> <%=lan.Price%></label>
                                            <input class="form-control" type="text" name="video_con_price" value="" readonly>
                                        </div>
                                        <div class="col-sm-12 col-md-6 col-lg-2 col-xl-2">
                                            <label class="col-form-label"><%=lan.Select%> <%=lan.Show%> <%=lan.Type%></label>
                                            <input class="form-control" type="text" name="video_con_price" value="" readonly>
                                        </div>
                                        <div class="col-sm-12 col-md-6 col-lg-1 col-xl-1">
                                            <label class="col-form-label"><%=lan.Status%></label>
                                            <input class="form-control" type="text" name="video_con_price" value="" readonly>
                                        </div>
                                    </div>
                                </div>
                            </div>



