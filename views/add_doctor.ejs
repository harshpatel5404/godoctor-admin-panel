<!DOCTYPE html>
<html lang="en">
  
<head>
    <%- include('./partials/head'); -%>
    
</head>
<body onload="startTime()">
    <%- include('./partials/header_sidebar'); -%>
    
    <!-- tap on top starts-->

        <!-- Page Sidebar Ends-->
        <div class="page-body">
          <div class="container-fluid">
            <div class="page-title">
              <div class="row">
                <div class="col-sm-6">
                  <h3><%=lan.Add%> <%= lan.Doctor %></h3>
                </div>
                <div class="col-sm-6">
                  <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/index"><i data-feather="home"></i></a></li>
                    <li class="breadcrumb-item"><%=lan.Dashboard%></li>
                    <li class="breadcrumb-item active"><%=lan.Add%> <%= lan.Doctor %></li>
                  </ol>
                </div>
              </div>
            </div>
          </div>
          <!-- Container-fluid starts-->
          <div class="container-fluid">
            <div class="row">
              <div class="col-sm-12">
                <div class="card">
                  <div class="card-body">
                    <form class="needs-validation" id="doc_form_click" novalidate="" action="/doctor/add_doctor_data" method="post" enctype="multipart/form-data">
                        <div class="row">
                            
                            <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 mb-3 form-group text-center">
                                <img id="file-preview" width="150" height="150" src="../images/dashboard/default/profile.png" alt="image">
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 mb-3 form-group text-center">
                                <img id="second-file-preview" width="150" height="150" src="../images/dashboard/default/profile.png" alt="image">
                            </div>

                            <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                                <label class="col-form-label"><%=lan.Name%></label>
                                <input class="form-control docin" id="customet_name" type="text" name="name" placeholder="<%=lan.Enter%> <%=lan.Name%>" required>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                                <label class="col-form-label"><%=lan.LOGO%></label>
                                <input class="form-control docin" style="height: 47px;" type="file" name="logo" accept="image/*" onchange="showPreview(event)" required>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                                <label class="col-form-label"><%=lan.Cover%> <%=lan.LOGO%></label>
                                <input class="form-control docin" style="height: 47px;" type="file" name="cover_logo" accept="image/*" onchange="secondshowPreview(event)" required>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                                <label class="col-form-label"><%=lan.Email%></label>
                                <input class="form-control docin" id="doc_email" type="email" name="email" placeholder="<%=lan.Enter%> <%=lan.Email%>" required>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                                <label class="col-form-label"><%=lan.Active%> <%=lan.Status%></label>
                                <select class="js-example-basic-single docin" name="status" required>
                                    <option value selected disabled><%=lan.Status%> <%=lan.List%></option>
                                    <option value="1"><%=lan.Active%></option>
                                    <option value="0"><%=lan.Deactive%></option>
                                </select>
                            </div>
                            <div class="col-sm-12 col-md-6 col-lg-4 col-xl-4 form-group">
                                <label class="col-form-label"><%=lan.Verified%> <%=lan.Status%></label>
                                <select class="js-example-basic-single docin" name="v_status" required>
                                    <option value selected disabled><%=lan.Verified%> <%=lan.Status%> <%=lan.List%></option>
                                    <option value="1"><%=lan.Verified%></option>
                                    <option value="0"><%=lan.Unverified%></option>
                                </select>
                            </div>
                            
                            <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6 form-group">
                                <label class="col-form-label"><%=lan.Title%></label>
