<style>
.loader-wrappers {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: white;
    z-index: 9999;
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 12px;
}

/* Dot style with colorful gradient and shadow */
.dot {
    width: 16px;
    height: 16px;
    border-radius: 50%;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
    animation: bounce 1s infinite ease-in-out;
}

/* Individual colorful dots */
.dot:nth-child(1) {
    background: #ff4d4d; /* red */
}
.dot:nth-child(2) {
    background: #ff9933; /* orange */
    animation-delay: 0.1s;
}
.dot:nth-child(3) {
    background: #ffd11a; /* yellow */
    animation-delay: 0.2s;
}
.dot:nth-child(4) {
    background: #33cc33; /* green */
    animation-delay: 0.3s;
}
.dot:nth-child(5) {
    background: #3399ff; /* blue */
    animation-delay: 0.4s;
}

@keyframes bounce {
    0%, 100% {
        transform: translateY(0);
        opacity: 1;
    }
    50% {
        transform: translateY(-12px);
        opacity: 0.7;
    }
}
</style>

